{
  "name": "Proxy Gateway API",
  "slug": "proxy",
  "api_id": "1",
  "org_id": "gateway_demo",
  "active": true,
  "use_keyless": false,
  "use_oauth2": false,
  "use_openid": true,
  "openid_options": {
    "providers": [
      {
        "issuer": "http://oidc:8080/realms/mockrealm",
        "client_ids": {
          "bW9ja19nYXRld2F5X2NsaWVudA==": "authn_gateway"
        }
      }
    ],
    "segregate_by_client": false
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true,
        "extended_paths": {
          "url_rewrites": [
            {
              "path": "/",
              "method": "GET",
              "match_pattern": "\\/pref\\/(.+)",
              "rewrite_to": "tyk://self/not_found",
              "triggers": [
                {
                  "on": "any",
                  "options": {
                    "header_matches": {
                      "X-Realm": {
                        "match_rx": ".+",
                        "reverse": false
                      }
                    }
                  },
                  "rewrite_to": "/$tyk_context.trigger-0-X-Realm-0/$1"
                }
              ]
            }
          ]
        }
      }
    }
  },
  "proxy": {
    "listen_path": "/pref/",
    "target_url": "http://localhost:8000",
    "strip_listen_path": true
  },
  "CORS": {
    "enable": true,
    "allow_credentials": true,
    "allowed_methods": ["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"],
    "allowed_headers": ["Origin", "Accept", "Content-Type", "Authorization", "User-Agent", "X-CSRF-Token", "X-Requested-With", "X-Realm"],
    "allowed_origins": [
       "http://localhost:5173",
       "http://localhost:8000",
       "http://tyk:8000",
       "*"
    ],
    "options_passthrough": false,
    "debug": true
  }
}
